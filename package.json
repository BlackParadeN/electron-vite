{
  "name": "electron-vite",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@7.12.0",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently \"pnpm dev:render\" \"pnpm dev:main\"",
    "dev:render": "vite",
    "dev:main": "pnpm build:main && electron .",
    "build": "pnpm build:render && pnpm build:main && electron-builder",
    "build:render": "vue-tsc --noEmit && vite build",
    "build:main": "tsc -p src/main --outDir dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "concurrently": "^7.4.0",
    "electron": "^21.0.1",
    "electron-builder": "^23.6.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^1.0.0"
  },
  "build": {
    "appId": "hyperion.bpn",
    "directories": {
      "output": "build"
    }
  }
}